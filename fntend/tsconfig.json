{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "../kbad/Frontend",
    "paths": {
      "@/*": [
        "../kbad/Frontend/src/*"
      ]
    },
    "strictNullChecks": true
  },
  "include": [
    "../kbad/Frontend/next-env.d.ts",
    "../kbad/Frontend/.next/types/**/*.ts",
    "../kbad/Frontend/**/*.ts",
    "../kbad/Frontend/**/*.tsx"
, "../kbad/Frontend/src/app/post/dashboard/[id].isx"  ],
  "exclude": [
    "../kbad/Frontend/node_modules"
  ]
}
